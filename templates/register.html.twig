{% extends "base.html.twig" %}

{% block title %}Register page{% endblock %}

{% block body %}
    <h1>
        Register
    </h1>
    {{ form_start(form, {attr: {'novalidate': 'novalidate'}, 'action': path('app_register')}) }}
    {{ form_row(form.email) }}
    {{ form_row(form.password) }}
    <button type="submit">Submit</button>
    {{ form_end(form) }}
{% endblock %}